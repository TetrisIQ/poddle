{
	"name": "poddle.network",
	"version": "1.0.0",
	"scripts": {
		"dev": "node server/dev/index.js",
		"build": "vite build",
		"start": "node server/index.js",
		"preview": "node server/index.js",
		"build:preview": "npm run build && npm run start",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"clearData": "rm data radata -dr"
	},
	"devDependencies": {
		"@beyonk/svelte-datepicker": "^12.0.1",
		"@playwright/test": "^1.35.1",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.0",
		"@types/express": "^4.17.17",
		"autoprefixer": "^10.4.14",
		"dayjs": "^1.11.9",
		"postcss": "^8.4.21",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.4",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.0",
		"tui-calendar": "^1.15.3",
		"typescript": "^5.1.6",
		"unique-username-generator": "^1.1.3",
		"uuid": "^9.0.0"
	},
	"type": "module",
	"dependencies": {
		"express": "^4.18.2",
		"gun": "^0.2020.1239",
		"svelte-local-storage-store": "^0.5.0",
		"svelte-overlay": "^1.4.1"
	}
}
